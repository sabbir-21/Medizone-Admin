<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:orientation="vertical">
		<ImageView
			android:id="@+id/imageview1"
			android:layout_width="150dp"
			android:layout_height="150dp"
			android:src="@drawable/logow_1"
			android:scaleType="fitXY"
			android:layout_gravity="center_horizontal" />
		<Button
			android:id="@+id/mzadmin"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:padding="8dp"
			android:background="#03A9F4"
			android:text="Medizone Admin"
			android:textSize="12sp"
			android:textColor="#FFFFFF" />
		<Button
			android:id="@+id/mzhome"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:padding="8dp"
			android:background="#03A9F4"
			android:text="Medizone Home"
			android:textSize="12sp"
			android:textColor="#FFFFFF" />
		<Button
			android:id="@+id/keyword"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:padding="8dp"
			android:background="#03A9F4"
			android:text="Keyword research tool"
			android:textSize="12sp"
			android:textColor="#FFFFFF" />
		<Button
			android:id="@+id/mzimage"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:padding="8dp"
			android:background="#03A9F4"
			android:text="Image selection"
			android:textSize="12sp"
			android:textColor="#FFFFFF" />
	</LinearLayout>
</LinearLayout>
